{
  "expo": {
    "name": "comic-client",
    "slug": "comic-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/comic-vine-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/comic-vine-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.yourcompany.comicclient",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/comic-vine-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "src/assets/fonts/PlusJakartaSans-ExtraBold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "73f5fda7-6bca-418f-a6ce-6f560af57bf5"
      }
    }
  }
}
